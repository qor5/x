{
  "name": "vuetifyxjs",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "comp": "npx tsx ./scripts/new-demo-doc.ts",
    "dev": "vitepress dev docs",
    "build": "vue-tsc --noEmit && vite build ",
    "build:lib": "cross-env NODE_ENV=lib babel src --out-dir lib --source-maps --extensions \".ts\",\".tsx\",\".snap\" --copy-files --no-copy-ignored --out-file-extension .mjs",
    "watch-build": "nodemon --watch src --ext ts,scss,vue --exec 'npm run build'",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "test:unit": "vitest",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@tiptap/core": "^2.11.3",
    "@tiptap/vue-3": "^2.11.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vueuse/core": "^11.0.3",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "lodash": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "roboto-fontface": "^0.10.0",
    "vue-i18n": "9.14.3",
    "vue-json-pretty": "^2.4.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.6.14",
    "vuetify-pro-tiptap": "https://github.com/qor5/vuetify-pro-tiptap.git"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@docsearch/css": "^3.6.1",
    "@docsearch/js": "^3.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitepress-code-preview/container": "1.0.8",
    "@vitepress-code-preview/plugin": "^1.0.6",
    "@vue/test-utils": "^2.4.6",
    "body-scroll-lock": "4.0.0-beta.0",
    "cross-env": "^7.0.3",
    "inquirer": "^10.2.2",
    "jsdom": "^24.1.0",
    "msw": "^2.3.5",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.45",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "sass-loader": "14.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.3.6",
    "vite-plugin-vuetify": "^2.0.3",
    "vitepress": "^1.3.4",
    "vitest": "^1.6.0",
    "vue": "^3.5.3",
    "vue-tsc": "^2.0.22"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vitepress-code-preview/container": "patches/@vitepress-code-preview__container.patch"
    }
  }
}
